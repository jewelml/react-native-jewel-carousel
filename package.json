{
  "name": "@jewel_ml/react-native-jewel-carousel",
  "title": "React Native Jewel Carousel",
  "version": "1.0.0",
  "description": "React Native component for creating a carousel with a JewelML personalizations",
  "exports": {
    ".": "./dist/index.js",
    "./ts": "./src/index.ts",
    "./ts/*.ts": "./src/*.ts",
    "./ts/*.tsx": "./src/*.tsx"
  },
  "files": [
    "README.md",
    "android",
    "ios",
    "dist",
    "src",
    "docs"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "./node_modules/.bin/jsdoc -c ./jsdoc.conf.json",
    "docs:markdown": "./node_modules/.bin/jsdoc2md --files ./dist/PersonalizationCarousel.js ./dist/SliderEntry.js ./dist/Models.js > docs/API.md",
    "postdocs": "npm run docs:markdown",
    "build": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "postbuild": "npm run docs",
    "publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jewelml/react-native-jewel-carousel.git",
    "baseUrl": "https://github.com/jewelml/react-native-jewel-carousel"
  },
  "keywords": [
    "react-native"
  ],
  "author": {
    "name": "Jan Bialostok",
    "email": "jan@jewelml.com"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": ">=16.8.1",
    "react-native": ">=0.60.0-rc.0 <1.0.x"
  },
  "devDependencies": {
    "@types/capitalize": "^2.0.0",
    "@types/react": "^18.0.35",
    "@types/react-native": "^0.71.5",
    "@types/react-native-snap-carousel": "^3.8.5",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "react": "^16.9.0",
    "react-native": "^0.61.5",
    "react-native-typescript-transformer": "^1.2.13",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "capitalize": "^2.0.4",
    "react-native-snap-carousel": "^3.9.1"
  }
}
