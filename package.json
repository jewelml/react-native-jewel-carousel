{
  "name": "@jewel_ml/react-native-jewel-carousel",
  "title": "React Native Jewel Carousel",
  "version": "1.0.0",
  "description": "React Native component for creating a carousel with a JewelML personalizations",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "react-native": "src/index.ts",
  "files": [
    "README.md",
    "android",
    "ios",
    "dist/src",
    "src",
    "docs"
  ],
  "scripts": {
    "test": "./node_modules/.bin/jest --clearCache && ./node_modules/.bin/jest",
    "docs": "./node_modules/.bin/jsdoc -c ./jsdoc.conf.json",
    "docs:markdown": "./node_modules/.bin/jsdoc2md --files ./dist/src/PersonalizationCarousel.js ./dist/src/SliderEntry.js ./dist/src/Models.js > docs/API.md",
    "postdocs": "npm run docs:markdown",
    "build": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "postbuild": "npm run docs",
    "publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jewelml/react-native-jewel-carousel.git",
    "baseUrl": "https://github.com/jewelml/react-native-jewel-carousel"
  },
  "keywords": [
    "react-native"
  ],
  "author": {
    "name": "Jan Bialostok",
    "email": "jan@jewelml.com"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-native": ">=0.71.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "@types/chance": "^1.1.3",
    "@types/enzyme": "^3.10.13",
    "@types/jest": "^29.5.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "chance": "^1.1.11",
    "enzyme": "^3.11.0",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.71.6",
    "react-native-typescript-transformer": "^1.2.13",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/capitalize": "^2.0.0",
    "@types/react": "^18.0.35",
    "@types/react-native": "^0.71.5",
    "@types/react-native-snap-carousel": "^3.8.5",
    "capitalize": "^2.0.4",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-reanimated": "^3.0.2",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-snap-carousel": "^3.9.1"
  }
}
